<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Albums</title>
</head>
<body>
    <h1>Welcome to the albums!</h1>
    <ol>
        <% albums.forEach(function(i) { %>
            <li>
                <a href='/albums/<%= i.id %>'><%= i.name %> - <%= i.artist %></a>
            </li>
       <% }) %>

    </ol>
</body>
</html>